        },
        "nwkSEncKey": {
          "type": "string",
          "description": "Network session encryption key (HEX encoded)."
        },
        "sNwkSIntKey": {
          "type": "string",
          "description": "Serving network session integrity key (HEX encoded)."
        },
        "fNwkSIntKey": {
