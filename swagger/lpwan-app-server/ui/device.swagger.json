      }
    },
    "gwDownlinkTiming": {
      "type": "string",
      "enum": [
        "IMMEDIATELY",
        "DELAY",
        "GPS_EPOCH"
      ],
      "default": "IMMEDIATELY",
      "description": " - IMMEDIATELY: Send the downlink immediately.\n - DELAY: Send downlink at the given delay (based on provided context).\n - GPS_EPOCH: Send at given GPS epoch value."
    },
