        },
        "nwkKey": {
          "type": "string",
          "title": "Network root key (HEX encoded).\nNote: For LoRaWAN 1.0.x, use this field for the LoRaWAN 1.0.x 'AppKey`!"
        },
        "appKey": {
          "type": "string",
